
@{
    ViewBag.Title = "页面管理列表";
    Layout = "~/Views/Shared/_LayoutBootstrapTable.cshtml";
}




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-sm-6">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>页面列表</h5>
                </div>

                <div class="ibox-content">
                    <div class="row row-lg">
                        <div class="col-sm-12">
                            <!-- Example Events -->
                            <div class="example-wrap">
                                <div class="example">

                                    <div class="btn-group hidden-xs" id="exampleTableEventsToolbar" role="group">
                                        <button type="button" class="btn btn-outline btn-default" id="btn_Edit">
                                            <i class="glyphicon glyphicon-pencil" aria-hidden="true"></i>
                                        </button>
                                        <button type="button" class="btn btn-outline btn-default" id="btn_Del">
                                            <i class="glyphicon glyphicon-trash" aria-hidden="true"></i>
                                        </button>
                                    </div>


                                    <table id="TableAdmin" data-height="auto" data-mobile-responsive="true"></table>

                                </div>
                            </div>
                            <!-- End Example Events -->
                        </div>
                    </div>
                </div>
            </div>
            <!-- End Example Events -->

        </div>
        <div class="col-sm-6">
            <div class="ibox float-e-margins">

                <div class="ibox-title">
                    <h5 id="editTitle">新增页面</h5>
                </div>

                <div class="ibox-content">
                    <form class="form-horizontal m-t" id="signupForm">

                        <div class="col-md-12">

                            <div class="form-group" id="thisId">
                                <label class="col-sm-3 control-label">ID：</label>
                                <div class="col-sm-9">
                                    <input type="text" id="id" name="id" class="form-control" value="">
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-3 control-label">页面名：</label>
                                <div class="col-sm-9">
                                    <input type="text" id="pageName" name="pageName" class="form-control" placeholder="首页">

                                </div>
                            </div>


                            <div class="form-group">
                                <label class="col-sm-3 control-label">页面地址：</label>
                                <div class="col-sm-9">
                                    <input type="text" id="pageUrl" name="pageUrl" class="form-control" placeholder="/Home/Index">

                                </div>
                            </div>
                            
                            <div class="form-group">
                                <label class="col-sm-3 control-label">页面分组：</label>
                                <div class="col-sm-9">
                                    <select id="groupId" name="groupId" class="form-control">
                                        
                                    </select>

                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label">左侧菜单：</label>
                                <div class="col-sm-9">
                                    <div class="switch">
                                        <div class="onoffswitch">
                                            <input type="checkbox" class="onoffswitch-checkbox" value="0" id="showMenu" name="showMenu">
                                            <label class="onoffswitch-label" for="showMenu">
                                                <span class="onoffswitch-inner"></span>
                                                <span class="onoffswitch-switch"></span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <label class="col-sm-3 control-label">页面锁：</label>
                                <div class="col-sm-9">
                                    <div class="switch">
                                        <div class="onoffswitch">
                                            <input type="checkbox" class="onoffswitch-checkbox" value="0" id="locked" name="locked">
                                            <label class="onoffswitch-label" for="locked">
                                                <span class="onoffswitch-inner"></span>
                                                <span class="onoffswitch-switch"></span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label">公开：</label>
                                <div class="col-sm-9">
                                    <div class="switch">
                                        <div class="onoffswitch">
                                            <input type="checkbox" class="onoffswitch-checkbox" value="0" id="jurisdiction" name="jurisdiction">
                                            <label class="onoffswitch-label" for="jurisdiction">
                                                <span class="onoffswitch-inner"></span>
                                                <span class="onoffswitch-switch"></span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <div class="form-group">
                            <div class="col-sm-10">
                            </div>
                        </div>

                        <div class="form-group">
                            <div class="col-sm-8 col-sm-offset-2">
                                <button class="btn btn-primary" type="button" id="btn_submit">保存</button>
                                <button class="btn btn-primary" type="reset" id="btn_reset">重置</button>
                            </div>
                        </div>
                    </form>

                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Script/Systenconfig-WebPagedAdmin-bom.min.js"></script>
